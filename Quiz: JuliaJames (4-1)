/*
 * Programming Quiz: JuliaJames (4-1)
 */

var x = 1;
var name;

while (x <= 20) {
                 if(x % 3 === 0) 
                    { name = "Julia"};
                 if (x % 5 === 0)
                    { name = "James" };
                 if (x % 3 === 0 && x % 5 === 0)
                    { name = "JuliaJames"};
                 if (x % 3 !== 0 && x % 5 !== 0 )
                    { name = x};
                console.log(name);
                x++;
}
